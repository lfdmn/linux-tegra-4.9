						reg = <0>;
                        status = "okay";
                        csi_in2: endpoint@4 {
                            port-index = <4>;
                            bus-width = <4>;
                            remote-endpoint = <&imx185_out4>; //should be sensor port under i2c endpoint
                            status = "okay";
                        };
                    };
                    port@1 {
